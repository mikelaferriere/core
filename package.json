{
  "name": "@mikelaferriere/core",
  "version": "1.0.10",
  "description": "Shared module containing logging, metrics and other modules",
  "main": "build/index.js",
  "repository": "https://github.com/mikelaferriere/core",
  "types": "./build/models",
  "scripts": {
    "build": "tsc",
    "deploy": "tsc",
    "postdeploy": "rm -rf node_modules",
    "lint": "eslint .",
    "format": "prettier --write .",
    "pre-commit": "npm run format && npm run lint && npm run build",
    "publish:local": "git tag v$npm_package_version && git push --tags && gh release create v$npm_package_version --generate-notes",
    "test": "jest ."
  },
  "files": [
    "./build",
    "./build/api",
    "./build/models"
  ],
  "keywords": [],
  "author": "Mike Laferriere",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/exporter-jaeger": "^1.14.0",
    "@opentelemetry/instrumentation": "^0.40.0",
    "@opentelemetry/instrumentation-http": "^0.40.0",
    "@opentelemetry/instrumentation-koa": "^0.34.6",
    "@opentelemetry/instrumentation-winston": "^0.31.4",
    "@opentelemetry/sdk-trace-base": "^1.14.0",
    "@opentelemetry/sdk-trace-node": "^1.14.0",
    "async-mqtt": "^2.6.3",
    "axios": "^1.4.0",
    "luxon": "^3.4.1",
    "winston": "^3.9.0",
    "winston-loki": "^6.0.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/luxon": "^3.3.1",
    "@types/node": "^18.6.0",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "jest": "^29.6.4",
    "prettier": "^2.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
